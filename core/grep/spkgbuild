name=grep
version=2.3.4
release=1
source="http://ftp.gnu.org/gnu/$name/$name-$version.tar.xz"

build() {
	cd $name-$version

	./configure --prefix=/usr --bindir=/bin
    make
    make DESTDIR=$PKG install
}
