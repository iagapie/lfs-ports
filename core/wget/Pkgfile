# Description:
# URL:
# Maintainer:
# Depends on: make-ca

name=wget
version=1.20.3
release=1
source=(https://ftp.gnu.org/gnu/$name/$name-$version.tar.gz)

build() {
	cd $name-$version

	./configure --prefix=/usr      \
            --sysconfdir=/etc  \
            --with-ssl=openssl
    make
    make DESTDIR=$PKG install
}
