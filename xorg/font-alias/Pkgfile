name=font-alias
version=1.0.3
release=1
source=(https://www.x.org/pub/individual/font/font-alias-$version.tar.bz2)

build() {
	cd font-alias-$version

    ./configure --prefix=/usr --sysconfdir=/etc --localstatedir=/var --disable-static
    make
	make DESTDIR=$PKG install
}
