# Description:
# URL:
# Maintainer:
# Depends on: xcursor-themes font-util 

name=encodings
version=1.0.5
release=1
source=(ftp://ftp.x.org/pub/individual/lib/encodings-$version.tar.bz2)

build() {
	cd encodings-$version

    ./configure --prefix=/usr --sysconfdir=/etc --localstatedir=/var --disable-static
    make
	make DESTDIR=$PKG install
}
