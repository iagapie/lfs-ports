# Description:
# URL:
# Maintainer:
# Depends on: fontconfig libxcb libxt 

name=libxmu
version=1.1.3
release=1
source=(ftp://ftp.x.org/pub/individual/lib/libXmu-$version.tar.bz2)

build() {
	cd libXmu-$version

    ./configure --prefix=/usr --sysconfdir=/etc --localstatedir=/var --disable-static
    make
	make DESTDIR=$PKG install
}
