# Description:
# URL:
# Maintainer:
# Depends on: pango cairo fontconfig gtk2 poppler-glib

name=graphviz
version=2.40.1
release=1
source=(http://graphviz.gitlab.io/pub/graphviz/stable/SOURCES/graphviz.tar.gz)

build() {
	cd $name-$version

	export LIBPOSTFIX=/
	./configure --prefix=/usr
	make
	make DESTDIR=$PKG install
}
