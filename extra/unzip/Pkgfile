# URL:
# Maintainer:
# Depends on:

name=unzip
version=6.0
release=1
source=(https://downloads.sourceforge.net/infozip/${name}${version//./}.tar.gz)

build() {
	cd ${name}${version//./}

  	make -f unix/Makefile generic
	make prefix=$PKG/usr MANDIR=$PKG/usr/share/man/man1 \
	 -f unix/Makefile install
}
